pipeline {
  agent { label 'windows-slave' }
  stages {
    stage('BuildUSA') {
      steps {        
        powershell '''
          mkdir peru          
        '''    
      }
    }
    stage('Package ArtifactUSA') {
      steps {
        bat '''
          dir
        '''  
        withAWS(credentials:'devops-s3',region:'us-east-1') {
            s3Download(file:'peru/', bucket:'belcorp-codedeploy', path:'SICC/QA-SICC/peru.txt', force:true)
        }
      }        
    }
    stage('Package ArtifactBRA') {
      steps {
        build job: 'DemoLinux'
      }        
    }
  }
}
