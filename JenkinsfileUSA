  agent { label 'windows-slave' }
  stages {
    stage('Build') {
      steps {        
        powershell '''
          mkdir peru          
          ls
        '''    
      }
      stage('Package EncoreUSA') {
      steps {        
        powershell '''            
          mkdir chile
          ls
        '''    
      }
      }
      stage('Package EncoreBRA') {
      steps {        
        powershell '''             
          mkdir bol
          ls
        '''    
      }
    }
    stage('Upload Package S3') {
      steps {
        
        withAWS(credentials:'devops-s3',region:'us-east-1') {
            s3Download(file:'.', bucket:'belcorp-codedeploy', path:'SICC/QA-SICC/jenkins-QA-SSICC-21.zip', force:true)
            s3Download(file:'.', bucket:'belcorp-codedeploy', path:'Encore/PRD-EncoreUSA/jenkins-PRD-EncoreUSA-8.zip', force:true)
        }        
      }        
    }
    
    
  }
